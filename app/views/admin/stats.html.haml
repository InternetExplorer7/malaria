%table
    %thead
        %tr
            %th School
            %th Name
            %th Registered
            %th Players
            %th Deaths
    %tbody
        - @communities.each do |community|
            %tr
                %td= community[:school]
                %td= community[:name]
                %td= community[:registered]
                %td= community[:players]
                %td= community[:deaths]
= render partial: 'users', locals: { users: @users }